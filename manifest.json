{
  "manifest_version": 2,
  "name": "Proton Calendar Keyboard Shortcuts",
  "version": "1.0",
  "description": "Adds keyboard shortcuts (e.g., next, previous, today) to Proton Calendar",
  "browser_specific_settings": {
    "gecko": {
      "id": "proton-calendar_keyboard-shortcuts"
    }
  },
  "icons": {
    "48": "icons/48.png"
  },
  "content_scripts": [
    {
      "matches": ["*://calendar.proton.me/*"],
      "js": ["index.js"]
    }
  ]
}
